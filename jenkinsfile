pipeline {
    agent any

    stages {
        stage('clone') {
            steps {
git branch: 'main', credentialsId: 'amazon_automation', url: 'https://github.com/amrithagk/HPE-CCBD-Summer-Project.git'
                
            }
        }

        stage('Build') {
            steps {
                echo"BUILDING.."
        bat script: 'mvn compile'
            }
        }

        stage('Test') {
            steps {
                echo"testing.."
bat script: 'mvn test -DBrowser=localchrome' 
            }
        }
    }
}
