pipeline {
    agent any

    stages {
        stage('clone') {
            steps {
                git branch: 'main', credentialsId: 'amazon_automation', url: 'https://github.com/amrithagk/HPE-CCBD-Summer-Project.git'
            }
        }

        stage('Build') {
            steps {
                echo "BUILDING.."
               dir(Automate to cart)
                   {   sh' python3 login.py'
                      sh'python product_search.py'
                       sh'python reviews.py'
}
}
}
}
}


