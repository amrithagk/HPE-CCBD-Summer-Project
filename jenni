pipeline {
    agent any

    stages {
        stage('clone') {
            steps {
                git branch: 'main', credentialsId: 'amazon_automation', url: 'https://github.com/amrithagk/HPE-CCBD-Summer-Project.git'
            }
        }

        stage('Build') {
            steps {
                echo "BUILDING.."
               dir('Automate to cart')
                   {   sh''' python3 steps/login.py&&
                      python3  steps/product_search.py&&
                       python3 steps/reviews.py'''
}
}
}
}
}


